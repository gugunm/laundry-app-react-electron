{
  "name": "toko-campus",
  "version": "1.0.0",
  "description": "point-of-sales toko campus",
  "author": "Gugun Mediamer",
  "license": "MIT",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "tailwind:css": "npx tailwindcss build src/styles/tailwind/tailwind.src.css -o src/styles/tailwind/tailwind.css",
    "build:dev": "yarn tailwind:css && webpack --mode=development",
    "build:prod": "yarn tailwind:css && webpack --mode=production",
    "dev-server": "yarn tailwind:css && webpack-dev-server --mode=development",
    "start": "yarn tailwind:css && cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "prod": "yarn tailwind:css && cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "build": "yarn tailwind:css && cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./release-builds --overwrite --platform=win32 --arch=x64 --icon=assets/icon.ico"
  },
  "dependencies": {
    "autoprefixer": "^9.8.4",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "easy-peasy": "^3.3.0",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "print-js": "^1.0.63",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-currency-format": "^1.0.0",
    "react-dates": "^21.8.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-times": "^3.1.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "electron": "^9.1.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-packager": "^15.0.0",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "file-loader": "^6.0.0",
    "image-webpack-loader": "^6.0.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "sweetalert2": "^9.15.2",
    "tailwindcss": "^1.4.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uniqid": "^5.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
